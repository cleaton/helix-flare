{
  "name": "helix-flare",
  "version": "1.1.5",
  "license": "MIT",
  "contributors": [
    "dan-lee <daniel@launchport.io>",
    "mosch <moritz@launchport.io>"
  ],
  "description": "GraphQL Helix for your Cloudflare Workers",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "envelop": [
        "dist/envelop.d.ts"
      ]
    }
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "NODE_OPTIONS=--experimental-vm-modules yarn jest --forceExit",
    "prepublishOnly": "yarn tsc --noEmit && yarn test && yarn build",
    "postinstall": "[ -z \"$RUN_POSTINSTALL\" ] || patch-package"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "keywords": [
    "graphql",
    "subscriptions",
    "sse",
    "cloudflare",
    "cloudflare-workers",
    "graphql-helix"
  ],
  "peerDependencies": {
    "@envelop/core": "1.6.2"
  },
  "peerDependenciesMeta": {
    "@envelop/core": {
      "optional": true
    }
  },
  "dependencies": {
    "event-iterator": "^2",
    "graphql": "^15",
    "graphql-helix": "1.10.3",
    "graphql-middleware": "^6.1.13"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.3.0",
    "@envelop/core": "1.6.5",
    "@graphql-tools/schema": "^8.3.1",
    "@graphql-tools/wrap": "^8.3.1",
    "@types/jest": "27.0.3",
    "esbuild": "^0.14.6",
    "graphql-sse": "^1.0.6",
    "jest": "^27.4.5",
    "jest-environment-miniflare": "2.0.0-rc.4",
    "jest-environment-node": "^27.4.4",
    "miniflare": "2.0.0-rc.4",
    "nanoevents": "6.0.2",
    "patch-package": "^6.4.7",
    "prettier": "^2.5.1",
    "source-map-support": "^0.5.21",
    "tiny-glob": "0.2.9",
    "ts-jest": "^27.1.2",
    "tsup": "^5.11.6",
    "typescript": "4.5.4",
    "undici": "^4.12.0"
  }
}
