{
  "name": "helix-flare",
  "version": "1.0.2",
  "license": "MIT",
  "contributors": [
    "dan-lee <daniel@launchport.io>",
    "mosch <moritz@launchport.io>"
  ],
  "description": "GraphQL Helix for your Cloudflare Workers",
  "type": "module",
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=16"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "NODE_OPTIONS=--experimental-vm-modules yarn jest --forceExit",
    "prepublishOnly": "yarn tsc --noEmit && yarn test && yarn build",
    "postinstall": "patch-package"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "keywords": [
    "graphql",
    "subscriptions",
    "sse",
    "cloudflare",
    "cloudflare-workers",
    "graphql-helix"
  ],
  "dependencies": {
    "event-iterator": "^2",
    "graphql": "^15",
    "graphql-helix": "1.10.2",
    "graphql-middleware": "^6"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.2.0",
    "@graphql-tools/schema": "^8.3.1",
    "@graphql-tools/wrap": "^8.3.1",
    "@types/jest": "27.0.3",
    "esbuild": "^0.13.15",
    "graphql-sse": "^1.0.6",
    "jest": "^27.3.1",
    "jest-environment-miniflare": "2.0.0-rc.2",
    "jest-environment-node": "^27.3.1",
    "miniflare": "2.0.0-rc.2",
    "nanoevents": "6.0.2",
    "patch-package": "^6.4.7",
    "prettier": "^2.4.1",
    "source-map-support": "^0.5.21",
    "tiny-glob": "0.2.9",
    "ts-jest": "^27.0.7",
    "tsup": "^5.8.1",
    "typescript": "4.5.2",
    "undici": "^4.10.2"
  }
}
