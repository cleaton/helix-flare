    "@types/jest": "27.0.2",
    "esbuild": "0.13.12",
    "graphql-sse": "1.0.5",
    "jest": "27.3.1",
    "jest-environment-node": "27.3.1",
    "miniflare": "2.0.0-next.3",
    "nanoevents": "6.0.2",
    "node-fetch": "3.0.0",
    "patch-package": "6.4.7",
    "prettier": "2.4.1",
    "ts-jest": "27.0.7",
    "tsup": "5.6.0",
